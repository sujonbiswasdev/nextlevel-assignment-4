model Category {
  id    String    @id @default(uuid()) 
  adminId String
  name  String @unique @db.VarChar(150)
  image   String
  createdAt      DateTime @default(now())
  updatedAt  DateTime   @updatedAt
  // relationship
  meals Meal[]
  user User @relation(fields: [adminId],references: [id],onDelete: Cascade)
  @@map("categories")
}